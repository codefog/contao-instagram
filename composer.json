{
    "name": "codefog/contao-instagram",
    "description": "Instagram bundle for Contao Open Source CMS",
    "keywords": [
        "contao",
        "instagram",
        "feed"
    ],
    "type": "contao-bundle",
    "license": "MIT",
    "authors": [
        {
            "name": "Codefog",
            "homepage": "https://codefog.pl"
        },
        {
            "name": "Kamil Kuzminski",
            "homepage": "https://github.com/qzminski",
            "role": "developer"
        }
    ],
    "require": {
        "php": "^8.1",
        "ext-json": "*",
        "contao/core-bundle": "^5.3"
    },
    "require-dev": {
        "contao/manager-plugin": "^2.0"
    },
    "conflict": {
        "contao/manager-plugin": "<2.0 || >=3.0"
    },
    "autoload": {
        "psr-4": {
            "Codefog\\InstagramBundle\\": "src/"
        }
    },
    "extra": {
        "contao-manager-plugin": "Codefog\\InstagramBundle\\ContaoManager\\Plugin"
    },
    "config": {
        "allow-plugins": {
            "contao-components/installer": true,
            "ocramius/package-versions": true,
            "contao-community-alliance/composer-plugin": true,
            "contao/manager-plugin": true,
            "php-http/discovery": true
        }
    }
}
